{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",

  "ReverseProxy": {
    "Routes": {

      "UdapLabsFhirStore-ss-route": {
        "ClusterId": "UdapLabsFhirStore-cluster",
        //NOTE:: udapPolicy means this endpoint is protected by UDAP
        "AuthorizationPolicy": "udapPolicy", 
        "MetaData": {
          "AccessToken": "gcp_ss_bearer_token"
        },
        "Match": {
          "Path": "/v1/projects/sandbox-348615/locations/us-central1/datasets/UdapLabsFhirDataset/fhirStores/UdapLabsFhirStore/{**remainder}"
        }
      },
      "UdapLabsFhirStore-ss-route-metadata": {
        "ClusterId": "self",
        "Match": {
          "Path": "/v1/projects/sandbox-348615/locations/us-central1/datasets/UdapLabsFhirDataset/fhirStores/UdapLabsFhirStore/fhir/.well-known/udap/{**remainder}"
        },
        "Transforms": [
          {
            "PathPattern": ".well-known/udap/{**remainder}"
          }
        ]
      },
      "UdapLabsFhirStore-ss-route-route-base": {
        "ClusterId": "UdapLabsFhirStore-cluster",
        "MetaData": {
          "AccessToken": "gcp_ss_bearer_token"
        },
        "Match": {
          "Path": "/v1/projects/sandbox-348615/locations/us-central1/datasets/UdapLabsFhirDataset/fhirStores/UdapLabsFhirStore/fhir/"
        }
      },


      "gFhirLab-route": {
        "ClusterId": "gFhirLab-cluster",
        //NOTE:: udapPolicy means this endpoint is protected by UDAP
        "AuthorizationPolicy": "udapPolicy",
        "MetaData": {
          "GCPKeyResolve": "gcp_joe_key_location",
          "UdapMetadata": "gcp_joe_udap_metadata"
        },
        "Match": {
          "Path": "/fhir/r4/{**remainder}"
        },
        "Transforms": [
          {
            "PathPattern": "/fhir/{**remainder}"
          }
        ]
      },
      "gFhirLab-route-wellknown": {
        "ClusterId": "self",
        "Match": {
          "Path": "/fhir/r4/.well-known/{**remainder}"
        },
        "Transforms": [
          {
            "PathPattern": ".well-known/{**remainder}"
          }
        ]
      },
      "gFhirLab-route-base": {
        "ClusterId": "gFhirLab-cluster",
        "MetaData": {
          "GCPKeyResolve": "gcp_joe_key_location"
        },
        "Match": {
          "Path": "/fhir/r4"
        },
        "Transforms": [
          {
            "PathSet": "/"
          }
        ]
      },
      "gFhirLab-route-metadata": {
        "ClusterId": "gFhirLab-cluster",
        "MetaData": {
          "GCPKeyResolve": "gcp_joe_key_location"          
        },
        "Match": {
          "Path": "/fhir/r4/metadata"
        },
        "Transforms": [
          {
            "PathSet": "/fhir/metadata"
          }
        ]
      }
    },    
    "Clusters": {
      
      "self": {
        "Destinations": {
          "destination1": {
            "Address": "https://localhost:7074"
          }
        }
      },

      "UdapLabsFhirStore-cluster": {
        "Destinations": {
          "destination1": {
            "Address": "https://healthcare.googleapis.com"
          }
        }
      },

      "gFhirLab-cluster": {
        "Destinations": {
          "destination1": {
            "Address": "https://healthcare.googleapis.com/v1beta1/projects/udap-idp/locations/us-west1/datasets/gFhirLab/fhirStores/fhirlabs_open"
          }
        }
      }

    }
  }
}
