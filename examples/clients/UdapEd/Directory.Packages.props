<Project>
  <PropertyGroup>
    <ManagePackageVersionsCentrally>false</ManagePackageVersionsCentrally>
    <Nullable>enable</Nullable>
    <RepositoryType>git</RepositoryType>
    <RepositoryUrl>https://github.com/JoeShook/udap-dotnet</RepositoryUrl>
    <Authors>Joseph Shook</Authors>
    <Copyright>Copyright Â© Joseph.Shook@Surescripts.com 2022</Copyright>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
    <PackageIcon>udap.logo.48x48.jpg</PackageIcon>
    <PackageTags>UDAP;FHIR;HL7;X509;</PackageTags>
    <Description>UDAP Learning tool is a part of the UDAP reference implementation for .NET.</Description>
    <TAG_NAME_ENV Condition="'$(TAG_NAME_ENV)' == ''">0.3.0</TAG_NAME_ENV>
    <VersionPrefix>$(TAG_NAME_ENV)</VersionPrefix>
    <UdapDotNetPackageVersion>0.2.*</UdapDotNetPackageVersion>
  </PropertyGroup>
  <ItemGroup>
  </ItemGroup>
  <ItemGroup>
    <PackageVersion Include="Duende.IdentityServer" Version="7.0.0" />
    <PackageVersion Include="Duende.IdentityServer.Storage" Version="7.0.0" />
    <PackageVersion Include="Serilog.AspNetCore" Version="7.0.0" />
  </ItemGroup>
</Project>